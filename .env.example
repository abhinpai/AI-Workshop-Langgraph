# API Configuration
API_BASE_URL=https://your-api-base-url.com/api
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/credentials.json

# Email Configuration
EMAIL_SENDER=your-email@example.com
EMAIL_PASSWORD=your-email-app-password
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587

# LangSmith Configuration
LANGSMITH_API_KEY=your-langsmith-api-key
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=your-project-name

# Database Configuration
POSTGRES_DB_CONNECTION_STRING=postgresql+psycopg://user:password@host:port/db_name
POSTGRES_USER=your-db-username
POSTGRES_PASSWORD=your-db-password
POSTGRES_HOST=your-db-host.database.azure.com
POSTGRES_PORT=5432
POSTGRES_DB=your-database-name
POSTGRES_SCHEMA=public

# Azure OpenAI Configuration
AZURE_OPEN_AI_KEY=your-azure-openai-key
AZURE_OPEN_AI_ENDPOINT=https://your-openai-instance.openai.azure.com/

# IAM Configuration
IAM_TOKEN_BASE_URL=https://your-iam-token-url.com/oauth2/token
IAM_CREDENTIALS=your-iam-credentials
